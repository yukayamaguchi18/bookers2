$(function() {
    if(range = 1){
        (".user-search-result").html("<%= escape_javascript(render partial: 'users/index', locals: {users: @records}) %>");
    } else if(range = 2){
        (".book-search-result").html("<%= escape_javascript(render partial: 'books/index', locals: {books: @records}) %>");
    } else{
    }
});

